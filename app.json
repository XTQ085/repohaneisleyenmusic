{
  "name": "Telegram Music",
  "logo": "https://te.legra.ph/file/e184200a18fd3fcd015a2.jpg", 
  "description": "Telegram'ınızın Grup Sesli Sohbeti'nde şarkı çalmak için Açık Kaynaklı bot.",
  "keywords": ["music", "voicechat", "telegram"],
  "repository": "https://github.com/XTQ085/repohaneisleyenmusic",
  "stack": "container",
  "env": {
    "SESSION_NAME": {
      "description": "Pyrogram oturum dizesi (Replit.com)",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "Bir bot belirteci @botfather ile alınız",
      "required": true
    },
    "BOT_NAME": {
      "description": "Müzik Oynatıcı Bot Adınız.",
      "required": false
    },
    "BOT_USERNAME": {
      "description": "Müzik Oynatıcı Bot Kullanıcı İsmi @ olmadan yazınız.",
      "required": true
    },
    "ASSISTANT_NAME": {
      "description": "Assistan kullanıcı adı @ olmadan yazınız.",
      "required": true
    },
    "OWNER_NAME": {
     "description": "Kendi kullanıcı adınız @ olmadan yazınız",
     "required": true
    },
    "DATABASE_URL": {
     "description": "Mongodb url si mongodb.com sitesinden alabilirsiniz",
     "required": true
    },
    "LOG_CHANNEL": {
     "description": "Log grubu veya log Kanal id",
     "required": true
    },
    "OWNER_ID": {
     "description": "Sahip hesabın id yazınız",
     "required": true
    },
    "START_IMAGE": {
     "description": "Botun start mesajı fotoğraf linki",
     "required": false
    },
    "API_ID": {
      "description": "my.telegram'dan uygulama kimliği.org/apps",
      "required": true
    },
    "API_HASH": {
      "description": "My.telegram'dan uygulama apı-hash.org/apps",
      "required": true
    },
    "SUDO_USERS": {
      "description": "Her yerde yönetici olarak sayılan kullanıcı kimliklerinin listesi (Müzik hesabınının id'si).",
      "required": true
    },
    "DURATION_LIMIT": {
      "description": "İndirmeler için en fazla ses süresi sınırı (minutes).",
      "required": true,
      "value": "55"
    },     
    "UPDATES_CHANNEL": {
      "description": "Kanal kullanıcı adı @ olmadan yazınız.",
      "required": true
    },
    "SUPPORT_GROUP": {
      "description": "Grup kullanıcı adı @ olmadan yazınız.",
      "required": true
    }
  }
}
